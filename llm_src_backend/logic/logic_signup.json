{
  "id": "logic_signup",
  "type": "logic",
  "description": "Register new user and return token/user object.",
  "inputs": ["email", "password", "name"],
  "outputs": ["token", "user"],
  "implementation": "If email is not taken, create user and return token/user object. Else, return error.",
  "tests": [
    {
      "id": "test_signup_valid",
      "type": "unit",
      "input": {"email": "new@example.com", "password": "password123", "name": "Ali"},
      "expected_output": {"token": "xyz789", "user": {"email": "new@example.com", "name": "Ali"}}
    },
    {
      "id": "test_signup_duplicate",
      "type": "unit",
      "input": {"email": "test@example.com", "password": "password123", "name": "Test"},
      "expected_output": {"error": "Email already exists"}
    }
  ]
}
